{
  "name": "zeit-log-integration",
  "version": 2,
  "alias": ["log-integration.now.sh"],
  "routes": [
    {
      "src": "/(?<icon>[^/]+).svg",
      "dest": "/routes/icon.js?icon=$icon"
    }
  ],
  "env": {
    "ROOT_URL": "https://log-integration.now.sh"
  },
  "build": {
    "env": {
      "ROOT_URL": "https://log-integration.now.sh"
    }
  },
  "builds": [
    {
      "src": "/routes/*.js",
      "use": "@now/node"
    },
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ]
}
